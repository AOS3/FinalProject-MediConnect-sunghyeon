<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SelectBox">
        <attr name="isSelected" format="boolean"/>
        <attr name="strokeColor" format="color"/>
        <attr name="textColor" format="color"/>
        <attr name="language" format="string" />
    </declare-styleable>

    <declare-styleable name="SelectCategory">
        <attr name="isCategorySelected" format="boolean"/>
        <attr name="categoryTitle" format="string"/>
        <attr name="categoryContent" format="string"/>
        <attr name="categoryStrokeColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="ExposedDropdownMenu">
        <attr name="menuTitle" format="string"/>
    </declare-styleable>

</resources>